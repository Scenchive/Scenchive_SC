import boto3

def get_s3_object_url(bucket_name, object_key):
    s3_client = boto3.client('s3')
    response = s3_client.generate_presigned_url(
        'get_object',
        Params={'Bucket': bucket_name, 'Key': object_key},
        ExpiresIn=3600  # URL의 만료 시간(초 단위)
    )
    return response

bucket_name = 'scenchive'
object_key = 'character_new01.gif'
object_url = get_s3_object_url(bucket_name, object_key)

print(f"The URL of the object 'character_new01.gif' in the bucket '{bucket_name}' is: {object_url}")